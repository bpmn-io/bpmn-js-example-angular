{
  "name": "bpmn-js-example-angular",
  "version": "0.0.0",
  "description": "An example how to integrate bpmn-js with an Angular application",
  "scripts": {
    "all": "run-s lint build test e2e",
    "build": "cd bpmn-js-app && yarn run build",
    "e2e": "cd bpmn-js-app && yarn run e2e -- --protractorConfig=e2e/protractor-ci.conf.js",
    "lint": "cd bpmn-js-app && yarn run lint",
    "postinstall": "cd bpmn-js-app && yarn install",
    "start": "cd bpmn-js-app && yarn start",
    "test": "cd bpmn-js-app && yarn test -- --watch=false --no-progress --browsers=ChromeHeadlessCI"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/bpmn-js-example-angular.git"
  },
  "keywords": [
    "Angular",
    "bpmn-js",
    "example"
  ],
  "license": "MIT",
  "devDependencies": {
    "yarn-run-all": "^3.1.1-yarn"
  }
}
