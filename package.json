{
  "name": "bpmn-js-example-angular",
  "version": "0.0.0",
  "description": "An example how to integrate bpmn-js with an Angular application",
  "scripts": {
    "all": "run-s lint build test e2e",
    "build": "npm run build -w bpmn-js-app",
    "dev": "npm run start",
    "e2e": "npm run e2e -w bpmn-js-app",
    "lint": "npm run lint -w bpmn-js-app",
    "start": "npm run start -w bpmn-js-app",
    "test": "npm test -w bpmn-js-app -- --watch=false --no-progress --browsers=ChromeHeadless"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/bpmn-js-example-angular.git"
  },
  "keywords": [
    "Angular",
    "bpmn-js",
    "example"
  ],
  "license": "MIT",
  "devDependencies": {
    "npm-run-all2": "^8.0.0"
  },
  "workspaces": [
    "bpmn-js-app"
  ]
}
