{
  "name": "bpmn-js-example-angular",
  "version": "0.0.0",
  "description": "An example how to integrate bpmn-js with an Angular application",
  "keywords": [
    "Angular",
    "bpmn-js",
    "example"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bpmn-io/bpmn-js-example-angular.git"
  },
  "license": "MIT",
  "scripts": {
    "all": "ng lint && npm run test && npm run e2e",
    "build": "ng build",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --protractor-config=./e2e/protractor-ci.conf.js",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test-ci": "ng test --watch=false --progress=false --browsers=ChromeHeadlessCI"
  },
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "bpmn-js": "3.0.4",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.1",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
